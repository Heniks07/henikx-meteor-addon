{
  "schemaVersion": 1,
  "id": "henikx-meteor",
  "version": "${version}",
  "name": "Henikx Meteor Addon",
  "description": "An addon by henikx adding some quality of life features",
  "authors": [
    "henikx"
  ],
  "contact": {
    "sources": "https://github.com/Heniks07/henikx-meteor-addon"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.henikx.addon.Meteor"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
